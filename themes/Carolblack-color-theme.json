			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#91b3e0"
			}
		},
		{
			"name": "HTML: Entities",
			"scope": [
				"constant.character.entity",
				"punctuation.definition.entity"
			],
			"settings": {
				"foreground": "#ab6526"
			}
		},
		{
			"name": "CSS: Selectors",
